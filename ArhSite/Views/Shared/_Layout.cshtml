<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - Schimdt Arhitecture Studio</title>
    <link rel="icon" href="@Url.Content("~/Content/Images/other_images/e5190e15-98d4-45be-95d4-96145723f08b.jpg")" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/jqueryval")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="overlay">

    </div>
    @{ Html.RenderPartial("_MenuSidebar"); }

    @RenderBody()
    @{ Html.RenderPartial("_Modal"); }


    @Scripts.Render("~/bundles/mainbootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
